<%- if node.presto['config']['type'] == 'coordinator' -%>
coordinator=true
node-scheduler.include-coordinator=false
discovery-server.enabled=true
<%- end -%>
<%- if node.presto['config']['type'] == 'worker' -%>
coordinator=false
<%- end -%>
<%- if node.presto['config']['type'] == 'pseudo' -%>
coordinator=true
node-scheduler.include-coordinator=true
discovery-server.enabled=true
<%- end -%>

http-server.http.port=<%= node.presto['config']['http_port'] %>
task.max-memory=<%= node.presto['config']['task_max_memory'] %>
discovery.uri=<%= node.presto['config']['discovery_uri'] %>
datasources=jmx,raptor,hive
